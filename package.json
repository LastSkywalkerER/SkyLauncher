{
  "name": "sky-launcher",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Minecraft Launcher for sky purposes",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "yarn typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "yarn build && electron-builder --dir",
    "build:win": "yarn build && electron-builder --win --x64",
    "build:mac": "yarn build && electron-builder --mac --arm64",
    "build:linux": "yarn build && electron-builder --linux --x64"
  },
  "dependencies": {
    "@doubleshot/nest-electron": "^0.2.5",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@melloware/react-logviewer": "^5.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.8",
    "@nestjs/microservices": "^10.3.8",
    "@xmcl/core": "^2.13.0",
    "@xmcl/installer": "^5.4.0",
    "autoprefixer": "^10.4.20",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "classnames": "^2.5.1",
    "electron-store": "8.2.0",
    "electron-updater": "^6.3.9",
    "inversify": "^6.0.2",
    "inversify-react": "^1.1.1",
    "minio": "^8.0.1",
    "postcss": "^8.4.47",
    "primeicons": "^7.0.0",
    "primereact": "^10.8.3",
    "react-hook-form": "^7.53.0",
    "react-router-dom": "^6.26.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tailwindcss": "^3.4.13",
    "ts-deepmerge": "^7.0.1",
    "undici": "^6.20.0",
    "zip-lib": "^1.0.4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^16.4.5",
    "electron": "^32.1.2",
    "electron-builder": "^25.1.7",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-node-polyfills": "^0.22.0"
  }
}
