{
  "name": "skylauncher",
  "version": "0.0.10-alpha",
  "license": "MIT",
  "description": "Minecraft Launcher for sky purposes",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://freshcraft.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "yarn typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "yarn build && electron-builder --dir",
    "build:win": "yarn build && electron-builder --win --x64",
    "build:mac": "yarn build && electron-builder --mac --arm64",
    "build:linux": "yarn build && electron-builder --linux --x64",
    "build:mac:freshcraft": "yarn build && electron-builder --mac --arm64  --config electron-builder.fresh-craft.yml",
    "build:win:freshcraft": "yarn build && electron-builder --win --x64 --config electron-builder.fresh-craft.yml"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.12.0",
    "@azure/msal-node": "^3.5.3",
    "@doubleshot/nest-electron": "^0.2.5",
