    "@electron-toolkit/utils": "^3.0.0",
    "@melloware/react-logviewer": "^5.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.8",
    "@nestjs/cqrs": "^11.0.0",
    "@nestjs/microservices": "^10.3.8",
    "@xmcl/core": "^2.14.0",
    "@xmcl/curseforge": "^2.2.0",
    "@xmcl/installer": "^6.0.1",
    "@xmcl/task": "^4.1.1",
    "@xmcl/user": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "classnames": "^2.5.1",
    "electron-store": "8.2.0",
    "electron-updater": "^6.3.9",
    "i18next": "^24.2.3",
    "inversify": "^6.0.2",
    "inversify-react": "^1.1.1",
    "jwt-decode": "^4.0.0",
    "minio": "^8.0.1",
    "nest-winston": "^1.10.2",
    "postcss": "^8.4.47",
    "primeicons": "^7.0.0",
    "primereact": "^10.8.3",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^6.26.2",
    "react-skinview3d": "5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tailwindcss": "^3.4.13",
    "ts-deepmerge": "^7.0.1",
    "undici": "^7.3.0",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "zip-lib": "^1.0.4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "electron": "^32.1.2",
    "electron-builder": "^25.1.7",
    "electron-vite": "^2.3.0",
